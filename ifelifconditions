failed_students = "Ashok"
Student_name = input("Enter your name to check Results:")
if Student_name in failed_students:
    print("sorry you have failed the exam.")
elif "Bharath" not in failed_students:
    print("Sorry next time better luck.")
else:
    print("Congratulations! you passed.")
